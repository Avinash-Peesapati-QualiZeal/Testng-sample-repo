{
  "testCaseId": "INT-123",
  "testCaseName": "valiadate header components",
  "description": "Test data for validating the presence and visibility of header components.",
  "priority": "Medium",
  "expectedResult": "User should be able to see the header components successfully",
  "headerComponents": [
    {
      "componentName": "Logo",
      "locatorType": "xpath",
      "locatorValue": "//header//img[@alt='Company Logo']",
      "expectedVisible": true,
      "edgeCases": [
        { "description": "Logo not loaded", "expectedVisible": false }
      ]
    },
    {
      "componentName": "Navigation Menu",
      "locatorType": "css",
      "locatorValue": "header nav.main-nav",
      "expectedVisible": true,
      "edgeCases": [
        { "description": "Menu collapsed on mobile", "expectedVisible": false }
      ]
    },
    {
      "componentName": "Search Bar",
      "locatorType": "id",
      "locatorValue": "header-search-input",
      "expectedVisible": true,
      "edgeCases": [
        { "description": "Search bar hidden for guest users", "expectedVisible": false }
      ]
    },
    {
      "componentName": "Profile Icon",
      "locatorType": "css",
      "locatorValue": "header .profile-icon",
      "expectedVisible": true,
      "edgeCases": [
        { "description": "Profile icon not shown when not logged in", "expectedVisible": false }
      ]
    },
    {
      "componentName": "Notifications",
      "locatorType": "xpath",
      "locatorValue": "//header//button[@aria-label='Notifications']",
      "expectedVisible": true,
      "edgeCases": [
        { "description": "Notifications hidden for new users", "expectedVisible": false }
      ]
    }
  ]
}
