// Test data for TC-N004: Maximum Character Limit for Name Field
[
  {
    "testCaseId": "TC-N004",
    "description": "Name field at maximum allowed character limit.",
    "preconditions": [
      "User is logged into the CRM system.",
      "User navigates to the contact creation screen."
    ],
    "input": {
      "name": "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" // 200 characters
    },
    "expectedResult": "System accepts the input and displays a confirmation message.",
    "postconditions": [
      "Contact is saved in the CRM system with the specified details."
    ],
    "priority": "Medium",
    "type": "Boundary",
    "locators": [
      { "field": "Name", "locator": "//input[@id='contactName']" },
      { "field": "SaveButton", "locator": "//button[@id='saveContact']" },
      { "field": "ConfirmationMessage", "locator": "//div[@class='confirmation-message']" },
      { "field": "ContactList", "locator": "//table[@id='contactList']" },
      { "field": "LogoutButton", "locator": "//button[@id='logout']" }
    ]
  },
  {
    "testCaseId": "TC-N004",
    "description": "Name field just below maximum allowed character limit.",
    "input": {
      "name": "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" // 199 characters
    },
    "expectedResult": "System accepts the input and displays a confirmation message."
  },
  {
    "testCaseId": "TC-N004",
    "description": "Name field just above maximum allowed character limit.",
    "input": {
      "name": "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" // 201 characters
    },
    "expectedResult": "System should not accept the input and should display an error message."
  },
  {
    "testCaseId": "TC-N004",
    "description": "Name field empty (minimum boundary).",
    "input": {
      "name": ""
    },
    "expectedResult": "System should not accept the input and should display an error message."
  }
